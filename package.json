{
  "name": "aymfx-ui",
  "version": "0.0.2",
  "type": "module",
  "description": "这是基于element-plus的组件库",
  "author": "aymfx",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "homepage": "https://aymfx.github.io/aymfx-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aymfx/aymfx-ui"
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/aymfx-ui.umd.cjs",
  "module": "./dist/aymfx-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/aymfx-ui.js",
      "require": "./dist/aymfx-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "build": "vue-tsc && vite build --config vite.build.config.ts",
    "preview": "vite preview",
    "dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.5.3",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.1",
    "vitepress": "1.0.0-rc.40",
    "vue-tsc": "^1.8.25"
  }
}
